{
    "presets": [
        "@babel/react",
        "@babel/typescript",
        "@babel/env"
    ],
    "plugins": [
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-proposal-object-rest-spread",
        [
            "import",
            {
                "libraryName": "antd",
                "libraryDirectory": "es",
                "style": "true"
            }
        ]
    ]
}